{
    "name": "dades/cms-bundle",
    "description": "Symfony bundle to easily create a CMS",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dades\\CmsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dades\\CmsBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Damien DE SOUSA",
            "email": "desousadamien30@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.7",
        "symfony/framework-bundle": "^5.4.9",
        "symfony/console": "^5.4.9",
        "symfony/monolog-bundle": "^3.8.0",
        "doctrine/orm": "^2.12.2",
        "doctrine/doctrine-bundle": "^2.7.0",
        "symfony/twig-bundle": "^5.4.8",
        "symfony/translation": "^5.4.9",
        "symfony/validator": "^5.4.8",
        "symfony-cmf/routing-bundle": "^2.5"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1.2",
        "dades/test-fixtures": "*@dev",
        "symfony/yaml": "^v5.4.10",
        "symfony/browser-kit": "^5.4.3"
    }
}
